{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.0.3",
        "way/generators": "dev-master",
        "sjdaws/vocal": "dev-master",
        "laravel/socialite": "3.0.*@dev",
        "martinbean/laravel-menu-builder": "dev-master",
        "illuminate/html" : "dev-master",
        "kris/laravel-form-builder": "~1.1",
        "doctrine/dbal": "~2.5.1",
        "darryldecode/cart": "dev-master",
        "caffeinated/flash": "1.0.*@dev",
        "barryvdh/laravel-debugbar": "~2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
